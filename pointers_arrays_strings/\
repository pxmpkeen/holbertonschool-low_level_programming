#include "main.h"
#include <stdio.h>
/**
 * _strspn - does smth strange
 * @s: string
 * @accept: string2
 *
 * Return: un
 */
unsigned int _strspn(char *s, char *accept)
{
	unsigned int i = 0, j;

	while (*(s + i))
	{
		j = 0;
		while (*(accept + j))
		{
			if (*(s + i) != *(accept + j))
				return (i + 1);
		}
	}
}
int main(void)
{
    char *s = "hello, world";
    char *f = "oleh";
    unsigned int n;

    n = _strspn(s, f);
    printf("%u\n", n);
    return (0);
}
